################################
#     GAS PROPERTIES
# -----------------------------

RHO_REF = 0.696864
P_REF = 1.0e5
T_REF = 500.0
U_INITIAL = 0.0 0.0 0.0 
#MU_REF = 3.0e-0
GAMMA = 1.4

################################
#      MESH
# -----------------------------

RESTART = ./laval.0100.msh
TRANSFORM_MESH
AMAX = 0.2
AMIN = 0.02

################################
#      SOLVER SETTINGS
# -----------------------------

#DT = 2.0e-6
CFL = 1.0
CFL_RAMP AFTER_ITER=1  INTERVAL_ITER=1  FACTOR_CFL=1.01  MAX_CFL=100.0

NSTEPS = 20000
CHECK_INTERVAL = 100
WRITE_RESTART = 2000
INITIAL_FLOWFIELD_OUTPUT = NO

### comment SPATIAL_SECOND_ORDER out to get first order upwind scheme 
#SPATIAL_SECOND_ORDER
### limiter for gradients 
GRAD_RECONSTRUCTION = SCHEME=GREENGAUSS LIMITER=YES EPS=0.01

### time integration 
TIME_INTEGRATION = FORWARD_EULER
#TIME_INTEGRATION = BACKWARD_EULER
#TIME_INTEGRATION = RK

### linear solvers 
LINEAR_SOLVER_NS = PETSC_GMRES

UNDER_RELAXATION = 0.4
LINEAR_SOLVER_NS_TRESHOLDS  MAX_ITER=30  ABS_RESID=1.0e-8  REL_RESID=1.0e-2
N_NEWTON_ITER = 1

#################################
# BOUNDARY CONSITIONS
# -------------------------------
inlet  = CBC_SUBSONIC_INLET 1.0 0.0 0.0 500.0 1.0e5
outlet = CBC_SUBSONIC_OUTLET 0.3e5
symm = SYMMETRY

WRITE_DATA FORMAT=TECPLOT NAME=fluid INTERVAL=2000 GEOM=ALL VARS= RHO vel RHOE temp press gamma residField sos Mach


