# GNUPLOT script to plot numerical solution vs. exact solution and compare different run types

# Screen output 
set terminal x11 persist
set key outside                        # put legend outside 
unset log                              # remove any log-scaling
unset label                            # remove any previous labels
set xtic auto                          # set xtics automatically
set ytic auto                          # set ytics automatically
set xrange [-1:1]
#set yrange [0.0:2e5]
set title "Strong expansion in shock tube: Temperature"
set xlabel "$x$"
set ylabel "$T$"

plot    "<sort -gk 1 ./shocktube_HLLC.dat"                   u 1:5 w l lw 1 lt 1 t 'HLLC', \
        "<sort -gk 1 ./shocktube_AUSMDV_H_NO_ENTR_FIX.dat"   u 1:5 w l lw 1 lt 2 t 'AUSMDV HAENEL NO ENTROPY FIX', \
        "<sort -gk 1 ./shocktube_AUSMDV_H.dat"               u 1:5 w l lw 1 lt 3 t 'AUSMDV HAENEL', \
        "<sort -gk 1 ./shocktube_HAENEL.dat"                 u 1:5 w l lw 1 lt 4 t 'HAENEL'

# Output for latex
set nokey
set notitle
set terminal epslatex color
set output "shocktube_expansion_comp.tex"
replot
