# gnuplot script to draw convergence 
set autoscale                        # scale axes automatically
unset log                              # remove any log-scaling
unset label                            # remove any previous labels
set xtic auto                          # set xtics automatically
set ytic auto                          # set ytics automatically
set yrange [0:0.01]
set title "skin friction"
set xlabel "Rex"
set ylabel "cf"
set terminal png
set output 'skinfriction.png'
plot    "<sort -gk 10 wallLam.dat" u 10:11 title 'analy lam' with points , \
        "<sort -gk 10 wallLam.dat" u 10:12 title 'emp turb' with points, \
        "<sort -gk 10 wallLam.dat" u 10:13 title 'JOE lam' with lines, \
        "<sort -gk 10 wallSA.dat"  u 10:13 title 'JOE SA' with lines, \
        "<sort -gk 10 wallSST.dat" u 10:13 title 'JOE SST' with lines, \
        "<sort -gk 10 wallWX.dat"  u 10:13 title 'JOE Wilcox' with lines

